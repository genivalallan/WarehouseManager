@model ListViewModel<Client>
@{
    ViewData["Title"] = "Lista de Clientes";
    ViewData["Entity"] = "Clientes";
    ViewData["Controller"] = "clients";
    ViewData["Items"] = Model.Items;
    ViewData["StartCounter"] = (Model.PagingInfo.Page - 1) * Model.PagingInfo.ItemsPerPage;
}

<partial name="_ListLayout" for="PagingInfo" view-data="ViewData" />
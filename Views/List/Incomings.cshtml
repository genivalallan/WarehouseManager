@model ListViewModel<Incoming>
@{
    ViewData["Title"] = "Lista de Entradas";
    ViewData["Entity"] = "Entradas";
    ViewData["Action"] = "Incomings";
    ViewData["Items"] = Model.Items;
    ViewData["StartCounter"] = (Model.PagingInfo.Page - 1) * Model.PagingInfo.ItemsPerPage;
}

<partial name="/Views/List/Layouts/_ListLayout.cshtml" for="PagingInfo" view-data="ViewData" />
@model PagingInfo
<h2 class="border border-dark rounded bg-secondary text-white text-center">
    Lista de @ViewData["Entity"]
</h2>
@if (Model == null || Model.ItemsPerPage == 0)
{
<div>
    <p class="bg-dark bg-gradient text-white text-center">
        Nenhum item encontrado.
    </p>
</div>
}
else
{
<table class="table table-bordered table-striped table-sm">
@if (ViewData["Items"] is IEnumerable<Client>)
{
    <partial name="/Views/Clients/_ListClients.cshtml" view-data="ViewData" />
}
else if (ViewData["Items"] is IEnumerable<Driver>)
{
    <partial name="/Views/Drivers/_ListDrivers.cshtml" view-data="ViewData" />
}
else if (ViewData["Items"] is IEnumerable<Enhancement>)
{
    <partial name="/Views/Enhancements/_ListEnhancements.cshtml" view-data="ViewData" />
}
else if (ViewData["Items"] is IEnumerable<Incoming>)
{
    <partial name="/Views/Incomings/_ListIncomings.cshtml" view-data="ViewData" />
}
else if (ViewData["Items"] is IEnumerable<Product>)
{
    <partial name="/Views/Products/_ListProducts.cshtml" view-data="ViewData" />
}
else if (ViewData["Items"] is IEnumerable<Shipping>)
{
    <partial name="/Views/Shippings/_ListShippings.cshtml" view-data="ViewData" />
}
else if (ViewData["Items"] is IEnumerable<Stock>)
{
    <partial name="/Views/Stocks/_ListStocks.cshtml" view-data="ViewData" />
}
else if (ViewData["Items"] is IEnumerable<Vehicle>)
{
    <partial name="/Views/Vehicles/_ListVehicles.cshtml" view-data="ViewData" />
}
    </table>
        <partial name="_PagingButtons" view-data="ViewData" />
}